<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@color/background">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:text="DAFTAR TRANSAKSI"
        android:textColor="@color/button"
        android:textSize="35dp"
        android:textStyle="bold" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="509dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="141dp"
        android:id="@+id/user_daftar_transaksi_RV"/>

    <ImageButton
        android:id="@+id/user_daftar_transaksi_btnHome"
        android:layout_width="111dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-5dp"
        android:layout_marginBottom="-11dp"
        android:background="@color/button"
        android:src="@drawable/ic_home" />

    <ImageButton
        android:id="@+id/user_daftar_transaksi_btnCart"
        android:layout_width="110dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="195dp"
        android:layout_marginBottom="-11dp"
        android:background="@color/button"
        android:src="@drawable/ic_cart" />

    <ImageButton
        android:id="@+id/user_daftar_transaksi_btnTicket"
        android:layout_width="110dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="216dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="-11dp"
        android:background="@color/button"
        android:src="@drawable/ic_ticket" />

    <ImageButton
        android:id="@+id/user_daftar_transaksi_btnProfile"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="314dp"
        android:layout_marginEnd="-20dp"
        android:layout_marginBottom="-11dp"
        android:background="@color/button"
        android:src="@drawable/ic_profile" />

</RelativeLayout>