<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="@color/background">

    <EditText
        android:id="@+id/user_register_event_NamaTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Nama Event"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/user_register_event_PenyelenggaraTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Penyelenggara"
        android:inputType="text" />

    <EditText
        android:id="@+id/user_register_event_TempatTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="103dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Tempat"
        android:inputType="text" />

    <EditText
        android:id="@+id/user_register_event_WaktuTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Waktu"
        android:inputType="text" />

    <EditText
        android:id="@+id/user_register_event_DeskripsiTxt"
        android:layout_width="250dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Deskripsi Acara"
        android:inputType="text" />

    <EditText
        android:id="@+id/user_register_event_UsernameEOTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="280dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Username EO"
        android:inputType="text" />

    <EditText
        android:id="@+id/user_register_event_PassEOTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="325dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Password EO"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/user_register_event_ConPassEOTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="369dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Confirm Password"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/user_register_event_HargaTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="412dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Harga Tiket"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/user_register_event_NorekTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="457dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Nomor Rekening"
        android:inputType="number" />

    <EditText
        android:id="@+id/user_register_event_AtasnamaTxt"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="545dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:ems="10"
        android:hint="Atas Nama"
        android:inputType="text" />

    <Spinner
        android:id="@+id/user_register_event_BankSpinner"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="501dp"
        android:layout_marginEnd="77dp"
        android:background="@android:color/white"
        android:entries="@array/bank" />

    <Button
        android:id="@+id/user_register_event_RegisBtn"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="8dp"
        android:background="@color/button"
        android:text="REGISTER"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/user_register_event_Back"
        android:layout_width="59dp"
        android:layout_height="61dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/ic_back" />

</RelativeLayout>